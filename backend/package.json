{
  "name": "@audiotailoc/backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm start:dev",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:simple": "ts-node src/main.simple.ts",
    "prebuild": "prisma generate",
    "build": "nest build",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "seed": "ts-node src/seed.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:unit": "jest --testPathPattern='\\.spec\\.ts$' --testPathIgnorePatterns='\\.e2e-spec\\.ts$'",
    "test:integration": "jest --testPathPattern='\\.integration\\.spec\\.ts$'",
    "test:e2e": "jest --testPathPattern='\\.e2e-spec\\.ts$'",
    "test:auth": "jest --testPathPattern='auth.*\\.spec\\.ts$'",
    "test:performance": "jest --testPathPattern='performance.*\\.spec\\.ts$'",
    "test:security": "jest --testPathPattern='security.*\\.spec\\.ts$'",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests",
    "test:setup": "node scripts/setup-test-db.js",
    "test:cleanup": "node scripts/cleanup-test-data.js",
    "start:prod": "node dist/main"
  },
  "prisma": {
    "seed": "ts-node src/seed.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^3.0.1",
