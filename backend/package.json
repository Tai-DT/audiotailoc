{
  "name": "@audiotailoc/backend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.x",
    "npm": ">=10.x"
  },
  "scripts": {
    "dev": "npm run start:dev",
    "start": "npx prisma generate && node dist/main.js",
    "start:dev": "nest start --watch",
    "start:simple": "ts-node src/main.simple.ts",
    "prebuild": "prisma generate",
    "build": "nest build",
    "typecheck": "tsc --noEmit",
    "typecheck:full": "tsc --noEmit --strict",
    "prisma:generate": "prisma generate",
    "postbuild": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "seed": "ts-node src/seed.ts",
    "create-admin": "npx ts-node ./scripts/create-admin.ts",
    "seed:simple": "node scripts/seed-simple.js",
    "backup:drive": "bash tools/backup-to-drive.sh",
    "restore:drive": "bash tools/restore-from-drive.sh",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepare": "sh -c \"if git rev-parse --git-dir >/dev/null 2>&1; then (cd \"$(git rev-parse --show-toplevel)\" && husky install); else echo '.git not found; skipping husky install'; fi\"",
    "pre-commit": "lint-staged",
    "test:integration": "mocha -r ts-node/register tests/**/*.spec.ts --exit",
    "test": "npm run test:integration"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/platform-socket.io": "^11.1.11",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.11",
    "@payos/node": "^2.0.3",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.3.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "@types/archiver": "^6.0.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/helmet": "^0.0.48",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.2",
    "@types/pdfkit": "^0.17.3",
    "@types/uuid": "^10.0.0",
    "archiver": "^7.0.1",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^7.2.4",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.8.0",
    "compression": "^1.8.1",
    "cron": "^4.3.4",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
