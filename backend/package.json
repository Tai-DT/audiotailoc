    "multer": "^2.0.2",
    "nestjs-pino": "^4.1.0",
    "nodemailer": "^7.0.5",
    "pino": "^9.3.1",
    "pino-http": "^10.1.0",
    "prom-client": "^15.1.3",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
